{
  "name": "Trello Id Extractor",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Trello Id Extractor",
  "permissions": [
    "https://trello.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://trello.com/*"],
      "run_at": "document_idle",
      "js": ["jquery.js", "code.js"]
    }
  ]
}
